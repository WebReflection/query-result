{
  "version": "0.2.0",
  "license": "ISC",
  "name": "query-result",
  "description": "Rethinking the $",
  "homepage": "https://github.com/WebReflection/query-result",
  "keywords": [
    "$",
    "jQuery",
    "Zepto",
    "like",
    "light",
    "lightweight",
    "utility",
    "DOM"
  ],
  "author": {
    "name": "Andrea Giammarchi",
    "web": "http://webreflection.blogspot.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/WebReflection/query-result.git"
  },
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "unpkg": "min.js",
  "scripts": {
    "build": "npm run cjs && npm run max && npm run min && npm run size",
    "cjs": "ascjs esm cjs",
    "max": "rollup -c",
    "min": "uglifyjs index.js --support-ie8 --comments=/^!/ -c -m -o min.js",
    "size": "cat min.js | wc -c;gzip -c9 min.js | wc -c;cat min.js | brotli | wc -c && rm -f min.js.br"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "ascjs": "^2.5.1",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3"
  }
}
